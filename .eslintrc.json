{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "linebreak-style": 0,
        "react/jsx-filename-extension": 0,
        "import/prefer-default-export": 0,
        "react/forbid-prop-types": 0,
        "class-methods-use-this": 0,
        "object-shorthand": 0,
        "react/prefer-stateless-function" : 0
    },
    "env": {
        "browser": true
    }
  }